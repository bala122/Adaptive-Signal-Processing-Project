\par \hich\af37\dbch\af31505\loch\f37 bnd = [0.6, 0.9];
\par \hich\af37\dbch\af31505\loch\f37 %fir1 uses a hamming window to give coefficients for the multiband filter
\par \hich\af37\dbch\af31505\loch\f37 b2 = fir1(order2,[low,bnd]);
